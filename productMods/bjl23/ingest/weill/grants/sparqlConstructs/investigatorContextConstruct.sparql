PREFIX rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>
PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>
PREFIX vivo:  <http://vivo.library.cornell.edu/ns/0.1#>
PREFIX mann:  <http://vivo.cornell.edu/ns/mannadditions/0.1#>
PREFIX xtor: <http://ingest.mannlib.cornell.edu/generalizedXMLtoRDF/0.1/> 
PREFIX owl:  <http://www.w3.org/2002/07/owl#>

CONSTRUCT {
    _:context rdf:type <http://www.aktors.org/ontology/support#Tangible-Thing> . # This is a temp. hack because we want this context node to be rejected as an invalid Tangible-Thing later. We don't want to keep it. 
    _:context <http://ingest.mannlib.cornell.edu/generalizedXMLtoRDF/0.1/PRINCIPAL_INVESTIGATOR> ?pi .
    ?grant <http://ingest.mannlib.cornell.edu/generalizedXMLtoRDF/0.1/AWARD_INVESTIGATORS_ROW> _:context .
    _:context <http://ingest.mannlib.cornell.edu/generalizedXMLtoRDF/0.1/AWARD_INVESTIGATORS_ROW> ?investigator .
} WHERE {
    ?grant <http://ingest.mannlib.cornell.edu/generalizedXMLtoRDF/0.1/AWARD_INVESTIGATORS> ?vacuous .
    ?vacuous <http://ingest.mannlib.cornell.edu/generalizedXMLtoRDF/0.1/AWARD_INVESTIGATORS_ROW> ?investigator .
    ?investigator <http://ingest.mannlib.cornell.edu/generalizedXMLtoRDF/0.1/PRINCIPAL_INVESTIGATOR> ?pi .
}
